{
	"name": "find-value-locations",
	"version": "2.1.0",
	"author": "Jordan Harband",
	"funding": {
		"url": "https://github.com/sponsors/ljharb"
	},
	"description": "Given an object, and a value, return a tuple of the property name, and the object on which it is an own property.",
	"license": "MIT",
	"main": "index.js",
	"sideEffects": false,
	"scripts": {
		"prepack": "npmignore --auto --commentLines=autogenerated",
		"prepublish": "not-in-publish || npm run prepublishOnly",
		"prepublishOnly": "safe-publish-latest",
		"lint": "eslint --ext=js,mjs .",
		"postlint": "tsc && attw -P",
		"pretest": "npm run lint",
		"test": "npm run tests-only",
		"posttest": "npx npm@'>= 10.2' audit --production",
		"tests-only": "nyc tape 'test/**/*.js'",
		"version": "auto-changelog && git add CHANGELOG.md",
		"postversion": "auto-changelog && git add CHANGELOG.md && git commit --no-edit --amend && git tag -f \"v$(node -e \"console.log(require('./package.json').version)\")\""
	},
	"repository": {
		"type": "git",
		"url": "git://github.com/ljharb/find-value-locations.git"
	},
	"keywords": [
		"find",
		"value",
		"property",
		"location",
		"prototype",
		"key",
		"object",
		"symbol",
		"locate",
		"location"
	],
	"dependencies": {
		"call-bound": "^1.0.3",
		"for-each": "^0.3.3",
		"gopd": "^1.2.0",
		"object-is": "^1.1.6",
		"protochain": "^1.0.5",
		"reflect.ownkeys": "^1.1.4",
		"safe-array-concat": "^1.1.3"
	},
	"devDependencies": {
		"@arethetypeswrong/cli": "^0.17.1",
		"@ljharb/eslint-config": "^21.1.1",
		"@ljharb/tsconfig": "^0.2.2",
		"@types/for-each": "^0.3.3",
		"@types/object-is": "^1.1.0",
		"@types/object.assign": "^4.1.0",
		"@types/reflect.ownkeys": "^1.1.0",
		"@types/tape": "^5.7.0",
		"auto-changelog": "^2.5.0",
		"encoding": "^0.1.13",
		"eslint": "=8.8.0",
		"has-proto": "^1.2.0",
		"in-publish": "^2.0.1",
		"npmignore": "^0.3.1",
		"nyc": "^10.3.2",
		"object.assign": "^4.1.5",
		"safe-publish-latest": "^2.0.0",
		"tape": "^5.9.0",
		"typescript": "next"
	},
	"testling": {
		"files": "test/index.js"
	},
	"engines": {
		"node": ">= 0.4"
	},
	"auto-changelog": {
		"output": "CHANGELOG.md",
		"template": "keepachangelog",
		"unreleased": false,
		"commitLimit": false,
		"backfillLimit": false,
		"hideCredit": true,
		"startingVersion": "2.0.4"
	},
	"publishConfig": {
		"ignore": [
			".github/workflows",
			"types"
		]
	}
}
